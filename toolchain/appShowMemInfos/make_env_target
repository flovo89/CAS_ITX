export TARGET=aarch64-linux-gnu
export TARGET_ROOTFS=/opt/embedded/firefly/rootfs/usr/local/bin
export LOCAL_INC=
export LOCAL_LIB=
export SYSTEM_INC=
export SYSTEM_LIB=
export EXTRA_CFLAGS=

